const matTabsAnimations = {
  /** Animation translates a tab along the X axis. */
  translateTab: (0, import_animations.trigger)("translateTab", [
    // Transitions to `none` instead of 0, because some browsers might blur the content.
    (0, import_animations.state)("center, void, left-origin-center, right-origin-center", (0, import_animations.style)({ transform: "none", visibility: "visible" })),
    // If the tab is either on the left or right, we additionally add a `min-height` of 1px
    // in order to ensure that the element has a height before its state changes. This is
    // necessary because Chrome does seem to skip the transition in RTL mode if the element does
    // not have a static height and is not rendered. See related issue: #9465
    (0, import_animations.state)("left", (0, import_animations.style)({
      transform: "translate3d(-100%, 0, 0)",
      minHeight: "1px",
      // Normally this is redundant since we detach the content from the DOM, but if the user
      // opted into keeping the content in the DOM, we have to hide it so it isn't focusable.
      visibility: "hidden"
    })),
    (0, import_animations.state)("right", (0, import_animations.style)({
      transform: "translate3d(100%, 0, 0)",
      minHeight: "1px",
      visibility: "hidden"
    })),
    (0, import_animations.transition)("* => left, * => right, left => center, right => center", (0, import_animations.animate)("{{animationDuration}} cubic-bezier(0.35, 0, 0.25, 1)")),
    (0, import_animations.transition)("void => left-origin-center", [
      (0, import_animations.style)({ transform: "translate3d(-100%, 0, 0)", visibility: "hidden" }),
      (0, import_animations.animate)("{{animationDuration}} cubic-bezier(0.35, 0, 0.25, 1)")
    ]),
    (0, import_animations.transition)("void => right-origin-center", [
      (0, import_animations.style)({ transform: "translate3d(100%, 0, 0)", visibility: "hidden" }),
      (0, import_animations.animate)("{{animationDuration}} cubic-bezier(0.35, 0, 0.25, 1)")
    ])
  ])
};
const _MatTabBodyPortal = class _MatTabBodyPortal extends import_portal.CdkPortalOutlet {
  constructor(componentFactoryResolver, viewContainerRef, _host, _document) {
    super(componentFactoryResolver, viewContainerRef, _document);
    this._host = _host;
    this._centeringSub = import_rxjs.Subscription.EMPTY;
    this._leavingSub = import_rxjs.Subscription.EMPTY;
  }
  /** Set initial visibility or set up subscription for changing visibility. */
  ngOnInit() {
    super.ngOnInit();
    this._centeringSub = this._host._beforeCentering.pipe((0, import_operators.startWith)(this._host._isCenterPosition(this._host._position))).subscribe((isCentering) => {
      if (this._host._content && isCentering && !this.hasAttached()) {
        this.attach(this._host._content);
      }
    });
    this._leavingSub = this._host._afterLeavingCenter.subscribe(() => {
      if (!this._host.preserveContent) {
        this.detach();
      }
    });
  }
  /** Clean up centering subscription. */
  ngOnDestroy() {
    super.ngOnDestroy();
    this._centeringSub.unsubscribe();
    this._leavingSub.unsubscribe();
  }
};
