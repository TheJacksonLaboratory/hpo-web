const DEFAULT_HORIZONTAL_ANIMATION_DURATION = "500ms";
const DEFAULT_VERTICAL_ANIMATION_DURATION = "225ms";
const matStepperAnimations = {
  /** Animation that transitions the step along the X axis in a horizontal stepper. */
  horizontalStepTransition: (0, import_animations.trigger)("horizontalStepTransition", [
    (0, import_animations.state)("previous", (0, import_animations.style)({ transform: "translate3d(-100%, 0, 0)", visibility: "hidden" })),
    // Transition to `inherit`, rather than `visible`,
    // because visibility on a child element the one from the parent,
    // making this element focusable inside of a `hidden` element.
    (0, import_animations.state)("current", (0, import_animations.style)({ transform: "none", visibility: "inherit" })),
    (0, import_animations.state)("next", (0, import_animations.style)({ transform: "translate3d(100%, 0, 0)", visibility: "hidden" })),
    (0, import_animations.transition)("* => *", (0, import_animations.group)([
      (0, import_animations.animate)("{{animationDuration}} cubic-bezier(0.35, 0, 0.25, 1)"),
      (0, import_animations.query)("@*", (0, import_animations.animateChild)(), { optional: true })
    ]), {
      params: { "animationDuration": DEFAULT_HORIZONTAL_ANIMATION_DURATION }
    })
  ]),
  /** Animation that transitions the step along the Y axis in a vertical stepper. */
  verticalStepTransition: (0, import_animations.trigger)("verticalStepTransition", [
    (0, import_animations.state)("previous", (0, import_animations.style)({ height: "0px", visibility: "hidden" })),
    (0, import_animations.state)("next", (0, import_animations.style)({ height: "0px", visibility: "hidden" })),
    // Transition to `inherit`, rather than `visible`,
    // because visibility on a child element the one from the parent,
    // making this element focusable inside of a `hidden` element.
