    this._position = "start";
    this._mode = "over";
    this._disableClose = false;
    this._opened = false;
    this._animationStarted = new import_rxjs.Subject();
    this._animationEnd = new import_rxjs.Subject();
    this._animationState = "void";
    this.openedChange = // Note this has to be async in order to avoid some issues with two-bindings (see #8872).
    new import_core.EventEmitter(
      /* isAsync */
      true
    );
    this._openedStream = this.openedChange.pipe((0, import_operators.filter)((o) => o), (0, import_operators.map)(() => {
    }));
    this.openedStart = this._animationStarted.pipe((0, import_operators.filter)((e) => e.fromState !== e.toState && e.toState.indexOf("open") === 0), (0, import_operators.mapTo)(void 0));
    this._closedStream = this.openedChange.pipe((0, import_operators.filter)((o) => !o), (0, import_operators.map)(() => {
    }));
    this.closedStart = this._animationStarted.pipe((0, import_operators.filter)((e) => e.fromState !== e.toState && e.toState === "void"), (0, import_operators.mapTo)(void 0));
